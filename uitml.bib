 type = {inproceedings},
 year = {2019},
 identifiers = {[object Object]},
 keywords = {feedforward neural network,physical activities,reinforcement learning,type 1 diabetes},
 pages = {1-20},
 volume = {14},
 issue = {November},
 id = {afb3a0df-caaf-3ce8-a6b1-bdab5567b34f},
 created = {2019-12-19T12:24:53.390Z},
