 type = {article},
 year = {2011},
 identifiers = {[object Object]},
 keywords = {Goodness of fit,Mellin transform (MT),log statistics,matrix-variate statistics,parameter estimation,probability density functions (PDFs),radar polarimetry,synthetic aperture radar (SAR)},
 id = {23b44a38-356b-3d30-aa31-5e0cc4127530},
