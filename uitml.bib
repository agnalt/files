 source = {IEEE Transactions on Image Processing},
 identifiers = {[object Object]},
 keywords = {Salient segmentation,connectivity,convolutional neural networks,salient instance-level segmentation},
 id = {65616c32-845a-3b2c-a973-bb0075900a6d},
 created = {2019-04-01T10:23:00.247Z},
 file_attached = {false},
 profile_id = {00330907-7fcb-39a3-a388-db79231d75d8},
